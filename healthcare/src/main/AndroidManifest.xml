                android:name="ir.madjeed.healthcare.gui.StarterActivity"
                android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name="ir.madjeed.healthcare.gui.RegisterActivity"/>
